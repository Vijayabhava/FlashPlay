{
    "manifest_version": 3,
    "name": "YouTube Speed Booster",
    "version": "1.1",
    "description": "Set YouTube playback speed up to 16x with a slider.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html"
    
    },
    "host_permissions": ["*://www.youtube.com/*"]
  }
  